<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="@dimen/dimen_radius_button_activity_register">
            </corners>
            <padding
                android:left="@dimen/dimen_paddingLeft_button_verfiy_activity_register"
                android:top="@dimen/dimen_paddingTop_button_verfiy_activity_register"
                android:right="@dimen/dimen_paddingRight_button_verfiy_activity_register"
                android:bottom="@dimen/dimen_paddingBottom_button_verfiy_activity_register"/>
            <solid
                android:color="@color/colorBackgroundVerfiyButtonRegisterActivity">
            </solid>
            <stroke
                android:color="@color/colorTextMainRegisterActivity"
                android:width="@dimen/dimen_stroke_button_activity_register">
            </stroke>
        </shape>
    </item>
    <item
        android:state_enabled="false">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="@dimen/dimen_radius_button_activity_register">
            </corners>
            <padding
                android:left="@dimen/dimen_paddingLeft_button_verfiy_activity_register"
                android:top="@dimen/dimen_paddingTop_button_verfiy_activity_register"
                android:right="@dimen/dimen_paddingRight_button_verfiy_activity_register"
                android:bottom="@dimen/dimen_paddingBottom_button_verfiy_activity_register"/>
            <solid
                android:color="@color/colorBackgroundVerfiyButtonRegisterActivity">
            </solid>
            <stroke
                android:color="@color/colorTextMainRegisterActivity"
                android:width="@dimen/dimen_stroke_button_activity_register">
            </stroke>
        </shape>
    </item>
</selector>
<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="@dimen/dimen_radius_button_activity_login">
            </corners>
            <solid
                android:color="@color/colorHyaline">
            </solid>
            <stroke
                android:color="@color/colorTextMainLoginActivity"
                android:width="@dimen/dimen_stroke_button_activity_login">
            </stroke>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="@dimen/dimen_radius_button_activity_login">
            </corners>
            <solid
                android:color="@color/colorTextMainLoginActivity">
            </solid>
            <stroke
                android:color="@color/colorTextMainLoginActivity"
                android:width="@dimen/dimen_stroke_button_activity_login">
            </stroke>
        </shape>
    </item>
</selector>